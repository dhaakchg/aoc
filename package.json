{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Advent of Code",
  "main": "./src/index.js",
  "author": "dhaakchg",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "eslint .",
    "create-day": "node bin/initNewPuzzleDay.js",
    "aoc": "node ./src/index.js",
    "2022": "npm run aoc 2022",
    "2023": "npm run aoc 2023",
    "2024": "npm run aoc 2024",
    "2025": "npm run aoc 2025",
    "current": "npm run aoc $(ls -1tr src/*/[0-9][0-9].js | tail -1 | sed -e 's#/# #g' -e 's/.js//g' | awk '{print $2,$3}')",
    "test:current": "jest --verbose --runTestsByPath $(ls -1tr src/*/[0-9][0-9].test.js | tail -1)"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "eslint": "^8.22.0",
    "eslint-plugin-jest": "^26.8.3",
    "glob": "11.1.0",
    "jest": "^28.1.3",
    "lodash": "^4.17.21"
  }
}
